tags:
  - projectMember
summary: 项目成员列表
description: 列出项目成员列表
operationId: "{{ uniqueOpId() }}"
parameters:
  - $ref: "@parameters/projectId.yml.njk"
  - $ref: "@parameters/accessToken.yml.njk"
responses:
  "200":
    description: 成功
    headers: 
      Access-Control-Allow-Origin:
        schema:
          type: string
          default: "*"
    content:
      application/json:
        schema:
          type: array
          items:
            $ref: "@schemas/simpleMemberInfo.yml.njk"
  "500":
    description:  失败
    headers: 
      Access-Control-Allow-Origin:
        schema:
          type: string
          default: "*"
    content:
      application/json:
        schema:
          $ref: "@schemas/errInfo.yml.njk"